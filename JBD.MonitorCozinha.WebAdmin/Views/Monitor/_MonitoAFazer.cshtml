@using JBD.MonitorCozinha.Domain.Enuns;
@model IEnumerable<JBD.MonitorCozinha.WebAdmin.Models.NumeroPedidoViewModel>

<div class="table col-lg-12 col-sm-12">
    @foreach (var item in Model)
    {
        if (item.IdStatusPedido == StatusPedidoEnum.AFazer)
        {
            <div class="float-left p-2"><span id="b"><button class="btn btn-outline-dark form-control" value="@item.IdStatusPedido" style="font-size:560px"><b>@item.NumeroPedido</b></button></span></div>
        }
    }
</div>
