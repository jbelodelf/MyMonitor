@model IEnumerable<JBD.MonitorCozinha.WebAdmin.Models.EmpresaViewModel>

    <table class="table table-striped table-bordered table-hover" id="tbListarEmpresas">
        <thead>
            <tr>
                @*<th>
                    @Html.DisplayNameFor(model => model.RazaoSocial)
                </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.NomeFantasia)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CNPJ)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.InscricaoEstadual)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.InscricaoMunicipal)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DataCadastro)
                </th>
                <th style="width: 100px;"></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    @*<td>
                        @Html.DisplayFor(modelItem => item.RazaoSocial)
                    </td>*@
                    <td>
                        @Html.DisplayFor(modelItem => item.NomeFantasia)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CNPJ)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.InscricaoEstadual)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.InscricaoMunicipal)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DataCadastro)
                    </td>
                    <td>
                        <a href="javascript:;" onclick="Empresa.ListarUnidade(@item.IdEmpresa)" title="Listar unidades"><i class="fa fa-list-alt"></i></a>
                        <a href="javascript:;" onclick="Empresa.Editar(@item.IdEmpresa)" title="Editar"><i class="fas fa-edit"></i></a>
                        <a href="javascript:;" onclick="Empresa.Excluir(@item.IdEmpresa)" title="Excluir"><i class="fas fa-trash"></i></a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
    <script src="~/lib/fontawesome/fontawesome-free-5.9.0-web/js/all.js"></script>